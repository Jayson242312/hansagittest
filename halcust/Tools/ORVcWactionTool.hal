global
function boolean GetBrokerNameORVc(Integer wn,var record ORVc ORr)
BEGIN
  record BrokerVc Brokerr;
  boolean res;
  res = false;

  Brokerr.BrNumber = ORr.BrNum;
  if(ReadFirstMain(Brokerr,1,true)) then begin
    ORr.BrName = Brokerr.BrName;
    res = true;
  end;//:MDS

  GetBrokerNameORVc = res;
return;
end;
