global
function boolean GetBrokerNameIVVc(Integer wn,var record IVVc IVr)
BEGIN
  record BrokerVc Brokerr;
  boolean res;
  res = false;

  Brokerr.BrNumber = IVr.BrNum; //MDS GREG-9
  if(ReadFirstMain(Brokerr,1,true)) then begin
    IVr.BrName = Brokerr.BrName;
    res = true;
  end;//:MDS

  GetBrokerNameIVVc = res;
return;
end;
